{
   "name": "emperor",
   "version": "5.12.0",
   "description": "The Emperor Discord bot.",
   "main": "dist/index.js",
   "scripts": {
      "build": "rm -fr dist/* && tsc",
      "watch": "tsc -w",
      "bot": "node -r dotenv/config dist/index.js",
      "prodbot": "NODE_ENV=production npm run bot",
      "start": "npm run build && npm run bot",
      "prodstart": "npm run build && NODE_ENV=production npm run bot",
      "db": "rm -f run_db.js && tsc run_db.ts && node run_db.js",
      "slash": "ts-node -r dotenv/config scripts/slash_update.ts",
      "scratch": "ts-node -r dotenv/config scripts/scratch.ts",
      "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
      "test": "echo \"Error: no test specified\" && exit 1"
   },
   "author": "Orbwebos",
   "license": "MIT",
   "dependencies": {
      "@sapphire/decorators": "^6.0.0",
      "@sapphire/framework": "^4.1.0",
      "@sapphire/utilities": "^3.11.0",
      "@types/uuid": "^8.3.4",
      "date-fns": "^2.25.0",
      "discord.js": "^14.7.1",
      "dotenv": "^10.0.0",
      "image-size": "^1.0.2",
      "imagescript": "^1.2.9",
      "node-schedule": "^2.0.0",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^5.0.11",
      "turndown": "^7.1.1",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@babel/eslint-parser": "^7.15.4",
      "@types/lodash": "^4.14.176",
      "@types/node": "^8.10.66",
      "@types/node-schedule": "^1.3.2",
      "@types/sqlite3": "^3.1.8",
      "@types/turndown": "^5.0.1",
      "@typescript-eslint/eslint-plugin": "^5.22.0",
      "@typescript-eslint/parser": "^5.22.0",
      "chrono-node": "^2.3.2",
      "date-fns-timezone": "^0.1.4",
      "eslint": "^7.32.0",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.0.0",
      "fuse.js": "^6.4.6",
      "prettier": "^2.6.2",
      "ts-node": "^10.4.0",
      "tslib": "^2.3.1",
      "typescript": "^4.6.4"
   }
}
